  <SQLQuery>
    <![CDATA[select distinct
v.CONT_HEADER_ID,
v.CERT_HEADER_ID,
v.APPL_HEADER_ID,
v.CONTRACT_NUM,
v.CERTIFICATION_NUMBER,
v.CUR_CERTIFICATION,
v.TAX_AMT,
nvl((v.CUR_CERTIFICATION+v.TAX_AMT),0) cur_cert_withtax,
v.VENDOR_NAME,
v.PROJECT_NUMBER,
v.PROJECT_NAME,
v.TOTAL_CONTRACT_SUM,
v.CURRENCY_CODE,
v.PAYMENT_DUE_DATE,
(select c.NOTE_TO_SUPPLIER from xxsc_contract_headers c where c.CONT_HEADER_ID=v.CONT_HEADER_ID and c.version_type='C' and rownum=1) as NOTE_TO_SUPPLIER, 
v.FLOW_WITH,
(select ua.user_name from xxsc_user_access ua where ua.user_id=v.FLOW_WITH and rownum=1) as approver,
v.CREATED_BY as buyer,
--contract
v.PREV_APPLICATION,
v.CUR_PAY_AMOUNT,
v.BAL_CONT_AMT,
--retention
v.TOTAL_RET,
v.PREV_RET,
v.CUR_RET,
v.BALANCE_RET,
--advance
v.TOTAL_ADV_AMOUNT,
v.PREV_ADV_REC,
v.CUR_ADV_REC,
v.BALANCE_ADV_REC,
--material
'0' totl_mtrl_amt,
v.PREV_MAT_ADV_AMT,
v.CUR_MAT_ADV_REC,
'0' blnc_mtrl_amt,
--net
nvl(v.TOTAL_CONTRACT_SUM-v.TOTAL_RET-v.TOTAL_ADV_AMOUNT-0,0) as net_cumm_amt,
nvl(v.PREV_APPLICATION-v.PREV_RET-v.PREV_ADV_REC-0,0) as net_prev_amt,
nvl(v.CUR_PAY_AMOUNT-v.CUR_RET-v.CUR_ADV_REC-0,0) as net_curnt_amt,
(select ah.APPLICATION_NUMBER from xxsc_pay_appl_headers ah where ah.APPL_HEADER_ID=v.APPL_HEADER_ID) as APPLICATION_NUMBER,
v.CERTIFICATION_DATE,
v.INVOICE_NUM,
v.INVOICE_DATE,
v.PAYMENT_TYPE_DISP,
v.PAYMENT_TERM,
v.COMMENTS

from 
xxsc_certification_headers_v v]]>
  </SQLQuery>
